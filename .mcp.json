{
    "mcpServers": {
        "laravel-boost": {
            "command": "php",
            "args": [
                "artisan",
                "boost:mcp"
            ]
        },
        "plan-director": {
            "command": "php",
            "args": [
                "artisan",
                "mcp:serve",
                "plan-director"
            ]
        }
    }
}